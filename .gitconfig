[commit]
	template = ~/.gitmessage.txt
[alias]
    s = status
    lo = log --format=oneline --no-decorate
    co = checkout
    cb = checkout -b
    copr = !sh -c 'git fetch upstream pull/$1/head:pr/$1 && git checkout pr/$1' -
    po = push origin HEAD
